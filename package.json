{
  "name": "stock-bot",
  "version": "0.1.0",
  "description": "PoC discord bot",
  "scripts": {
    "compile": "tsc",
    "deploy": "npm run compile && node ./build/cmd/delete-commands.js && node ./build/cmd/create-commands.js",
    "server": "npm run compile && node ./build/cmd/server.js",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "github.com/laupski/stock-bot"
  },
  "keywords": [
    "node",
    "discord",
    "stock"
  ],
  "author": "laupski",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^0.7.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "alphavantage": "^2.3.0",
    "discord-api-types": "^0.24.0",
    "discord.js": "^13.2.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "2.4.1",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": "16.x"
  }
}
